<h3 id="document-list-heading" class="accessible-hidden">List of files deposited in <%= t('sufia.product_name') %> that match your search criteria</h3>
<% if display_as_grid? %>
  <ul id="documents" class="search-results-grid" aria-labeled-by="document-list-heading">
  <% @document_list.map(&:to_model).each do |document| %>
    <%= render 'thumbnail_result_tile', document: document %>
  <% end %>
  </ul>
<% else %>
  <ul id="documents" class="container-fluid search-results-list" aria-labeled-by="document-list-heading">
    <%= render @document_list.map(&:to_model), :as => :document %>
  </ul>
<% end %>
<%= debug @document_list %>
