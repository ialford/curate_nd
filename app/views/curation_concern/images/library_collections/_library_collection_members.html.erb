<% if curation_concern.library_collection_members.any? %>
  <div class="library-collection-members">
    <h2>Collection Members</h2>
    <ul>
      <% curation_concern.library_collection_members.each do |member| %>
        <li><%= link_to(member, common_object_path(member.noid)) %></li>
      <% end %>
    </ul>
  </div>
<% end %>
