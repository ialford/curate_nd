<%= simple_form_for(@temporary_access_token) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sha %>
    <%= f.input :noid %>
    <%= f.input :issued_by %>
    <%= f.input :used %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
