<%= simple_form_for([:admin, @temporary_access_token]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :noid %>
    <% if @temporary_access_token.persisted? %>
      <%= f.input :used %>
    <% end %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
